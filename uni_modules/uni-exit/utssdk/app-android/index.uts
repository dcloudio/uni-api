import { ExitOptions, ExitSuccess, ExitCompleteCallback, Exit } from "../interface.uts"


/**
 * 实现uni.exit
 */
export const exit : Exit = function (options: ExitOptions | null) {
	let ret : ExitSuccess ={
		errMsg: "exit:ok"
	}
	options?.success?.(ret)
	options?.complete?.(ret)
	UTSAndroid.exit()
}

